<%= form_for([:admin, @yard]) do |y| %>
  <%= render "shared/error_messages.html.erb", objects: y.object %>
  <%= y.label :code %>
  <%= y.text_field :code, class: "form-control" %>
  <%= y.label :type_yard %>
  <%= y.select :type_yard, options_for_select([["5", "5"], ["7", "7"],["11", "11"]]) %>
  <%= y.submit t("home.save"), class: "btn btn-primary" %>
<% end %>
