<%= search_form_for @q, url: root_path, class: "form-group d-flex mb-0 form-search-yard" do |f| %>
  <%= f.search_field :location_cont, class: "form-control input-search-yard", placeholder: t("base.search") %>
  <%= button_tag(type: "submit", class: "btn btn-dark button btn1") do %>
    <i class="fa fa-search"></i>
  <% end %>
<% end %>
